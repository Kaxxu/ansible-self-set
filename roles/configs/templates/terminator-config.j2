[global_config]
  borderless = True
  enabled_plugins = APTURLHandler, LaunchpadCodeURLHandler, LaunchpadBugURLHandler
  inactive_color_offset = 1.0
  suppress_multiple_term_dialog = True
  title_inactive_bg_color = "#000000"
  title_inactive_fg_color = "#ffffff"
  title_use_system_font = False
  window_state = maximise
[keybindings]
  next_profile = <Primary><Shift>z
  next_tab = <Primary><Shift>Tab
  prev_tab = <Primary><Shift>Tab
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[default]]
    copy_on_selection = True
    cursor_color = "#ff0000"
    cursor_color_fg = False
    font = Monospace 10
    foreground_color = "#ffffff"
    palette = "#2e3436:#cc0000:#4e9a06:#73d216:#3465a4:#75507b:#06989a:#d3d7cf:#555753:#ef2929:#8ae234:#fce94f:#729fcf:#f57900:#52b5ec:#eeeeec"
    scrollback_infinite = True
  [[white]]
    background_color = "#ffffff"
    copy_on_selection = True
    cursor_color = "#a40000"
    cursor_color_fg = False
    foreground_color = "#000000"
    palette = "#5c3566:#fb4934:#a40000:#fce94f:#204a87:#10c7fb:#73d216:#a40000:#a40000:#fb4934:#555753:#c4a000:#83a598:#d3869b:#8ec07c:#ebdbb2"
    scrollback_infinite = True